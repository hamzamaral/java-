	static int overloading1(int a , double b) {
		return a;
		
	}
	
	static int overloading1(double b,int a) {
		return a;
	}
	public static void main(String[] args) {
		
		int k=overloading1(5,6.2);
		int y=overloading1(4,7.3);
		System.out.println(k + "\n" + y);
		
	}
	
