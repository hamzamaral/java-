		long a= 5465125;
				String deðer1= String.valueOf(a);
				String deðer2=long.tostring(a);
		float b =311.123f;
		 String deðer3=String.valueOf(b);
		 String deðer4= float.tostring(b);
		byte c=11;
		 String deðer5= String.valueOf(c);
		String deðer6=byte.tostring(c);
		 
		System.out.println(deðer1);
